class vehiculo:
    def mover(self):
        raise NotImplementederror ("este metodo debe estar implementado por las subclase. ")

class coche(vehiculo):
    def mover(self):
        print("el coche esta conduciendo por la carretera.")
        
class vicicleta(vehiculo):
    def mover(self):
        print("la visicleta esta pedaleando por el carril bici.")
        
def mover_vehiculos(vehiculos):
   for vehiculo in vehiculos:
       vehiculo.mover()
       
vehiculos = [coche(),vicicleta(),coche()]

mover_vehiculos(vehiculos)
