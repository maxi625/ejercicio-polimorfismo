class Animal:
    def hacerSonido(self):
        raise NotImplementedError("Este m√©todo debe ser implementado por las subclases.")

class Perro(Animal):
    def hacerSonido(self):
        return "Guau"

class Gato(Animal):
    def hacerSonido(self):
        return "Miau"

animales = [
    Perro(),
    Gato(),
    Perro(),
    Gato(),
    Perro()
]

for i, animal in enumerate(animales, start=1):
    print(f"Animal {i}: {animal.hacerSonido()}")
